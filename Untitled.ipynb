   "metadata": {},
   "outputs": [],
   "source": [
    "\n",
    "# Generator function for the cube of numbers (power of 3)\n",
    "def gencubes(n):\n",
    "    print(\"test 1:  \",n)\n",
    "    for num in range(n):\n",
    "        print(\"test :  \",num)\n",
    "        yield num**3"
   ]
  },
